<Page x:Class="EATS_kitchen.Activation"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:EATS_kitchen"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="Activation">

    <Grid Background="White">
        <Grid HorizontalAlignment="Left" Height="389" Margin="10,41,0,0" VerticalAlignment="Top" Width="675">
            <Frame x:Name="ActiveControl" NavigationUIVisibility="Hidden" Grid.ColumnSpan="2"></Frame></Grid>
        <Button x:Name="BtnSave" Content="저장" HorizontalAlignment="Left" Height="50" Margin="704,41,0,0" VerticalAlignment="Top" Width="84" FontSize="30" Click="BtnSave_Click"/>
        <Button x:Name="BtnCancel" Content="취소" HorizontalAlignment="Left" Height="50" Margin="704,114,0,0" VerticalAlignment="Top" Width="84" FontSize="25" Click="BtnCancel_Click"/>

    </Grid>
</Page>
